{
  "compilerOptions": {
    "jsx": "react",
    "target": "ESNext", 
    "module": "ESNext", 
    "lib": ["ESNext", "DOM"], 
    "declaration": true,
    "declarationMap": true, 
    "outDir": "./dist",  
    "rootDir": "./src", 
    "strict": true, 
    "esModuleInterop": true, 
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
